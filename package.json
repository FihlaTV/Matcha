{
  "name": "matcha",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint src --color",
    "client": "NODE_ENV=development webpack-dev-server --client-log-level 'none' --progress --config webpack.config.js",
    "server": "NODE_ENV=development DEBUG=matcha* nodemon --watch config/server --watch src/lib --watch src/server --exec \"babel-node src/server\"",
    "resetdb": "NODE_ENV=development RESET_DB=true DEBUG=matcha* babel-node src/server",
    "loadusers": "NODE_ENV=development LOAD_USERS=true DEBUG=matcha* babel-node src/server",
    "coverage": "NODE_ENV=testing nyc yarn srv:test",
    "test:server": "NODE_ENV=testing DEBUG=matcha* mocha --compilers js:babel-core/register --require babel-polyfill --reporter spec \"./src/server/**/__test__/*.js\" ",
    "test:client": "NODE_ENV=testing mocha --require ignore-styles --require babel-core/register --reporter spec \"./src/client/**/__test__/*.js\" ",
    "bundle": "NODE_ENV=production webpack -p --config webpack.config.js --profile --json > stats.json; webpack-bundle-analyzer ./stats.json ./build --report --mode=static"
  },
